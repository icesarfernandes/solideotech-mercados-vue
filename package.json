{
  "name": "solideotech-mercados",
  "version": "0.1.0",
  "description": "SolideoTech Mercados",
  "author": "Italo Cesar Fernandes <italosantos.fernandes@gmail.com>",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "webpack-dashboard -t 'Project' -- webpack-dev-server",
    "clean": "shx rm -rf ./build",
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
    "serve": "http-server -o -c 1 -a localhost",
    "test": "karma start karma.conf.js",
    "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/build.css ] -e src/main.js | uglifyjs -c warnings=false -m > dist/build.js",
    "lint:js": "eslint ./src ./webpack ./*.js -f table --ext .js --ext .jsx",
    "lint:scss": "stylelint ./src/**/*.scss --syntax scss",
    "lint": "npm run lint:js && npm run lint:scss",
    "update:package": "npm i npm-check-updates && ncu -u && npm installnpm"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "express": "^4.16.3",
    "vue": "^2.0.1",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.0.0",
    "bootstrap-datepicker": "^1.7.1",
    "chart.js": "^2.7.1",
    "datatables": "^1.10.13",
    "easy-pie-chart": "^2.1.7",
    "ember-cli": "^3.4.3",
    "file-loader": "^2.0.0",
    "fullcalendar": "^3.6.2",
    "jquery": "^3.2.1",
    "jquery-sparkline": "^2.4.0",
    "jvectormap": "^2.0.4",
    "load-google-maps-api": "^1.0.0",
    "lodash": "^4.17.4",
    "masonry-layout": "^4.2.0",
    "moment": "^2.19.1",
    "nodejs": "0.0.0",
    "perfect-scrollbar": "^1.1.0",
    "popper.js": "^1.12.6",
    "skycons": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^5.2.0",
    "envify": "^4.1.0",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.2.1",
    "karma": "^3.0.0",
    "karma-browserify": "^5.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.32",
    "npm-run-all": "^4.1.3",
    "phantomjs-prebuilt": "^2.1.3",
    "proxyquireify": "^3.0.1",
    "uglify-js": "^3.4.9",
    "vueify": "^9.0.0",
    "watchify": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "copy-webpack-plugin": "^4.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "2.14.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.2",
    "style-loader": "^0.23.0",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.20.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.9"
  },
  "engines": {
    "node": ">= 8"
  }
}
